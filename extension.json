{
	"name": "YetiTweaks",
	"version": "1.1.0",
	"author": [
		"Jayden Bailey",
		"TehKittyCat",
		"Winter"
	],
	"url": "https://yeti.wiki",
	"descriptionmsg": "yetitweaks-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "other",
	"MessagesDirs": {
		"YetiWikisMessages": [
			"i18n/yetiwikis"
		],
		"YetiWikisOverrides": [
			"i18n/yetiwikisoverrides"
		]
	},
	"requires": {
		"MediaWiki": ">= 1.37.0"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\YetiTweaks\\": "includes/"
	},
	"AuthManagerAutoConfig": {
		"preauth": {
			"MediaWiki\\Extension\\YetiTweaks\\StopForumSpam\\GloopPreAuthenticationProvider": {
				"class": "MediaWiki\\Extension\\YetiTweaks\\StopForumSpam\\GloopPreAuthenticationProvider",
				"sort": 5
			}
		}
	},
	"Hooks": {
		"BeforePageDisplay": "MediaWiki\\Extension\\YetiTweaks\\YetiTweaksHooks::onBeforePageDisplay",
		"GetLocalURL::Internal": "MediaWiki\\Extension\\YetiTweaks\\YetiTweaksHooks::onGetLocalURLInternal",
		"LocalFilePurgeThumbnails": "MediaWiki\\Extension\\YetiTweaks\\YetiTweaksHooks::onLocalFilePurgeThumbnails",
		"MessageCacheFetchOverrides": "MediaWiki\\Extension\\YetiTweaks\\YetiTweaksHooks::onMessageCacheFetchOverrides",
		"OpenSearchUrls": "MediaWiki\\Extension\\YetiTweaks\\YetiTweaksHooks::onOpenSearchUrls",
		"ResourceLoaderRegisterModules": "MediaWiki\\Extension\\YetiTweaks\\YetiTweaksHooks::onResourceLoaderRegisterModules",
		"SkinAddFooterLinks": "MediaWiki\\Extension\\YetiTweaks\\YetiTweaksHooks::onSkinAddFooterLinks",
		"SkinCopyrightFooter": "MediaWiki\\Extension\\YetiTweaks\\YetiTweaksHooks::onSkinCopyrightFooter",
		"TestCanonicalRedirect": "MediaWiki\\Extension\\YetiTweaks\\YetiTweaksHooks::onTestCanonicalRedirect",
		"TitleSquidURLs": "MediaWiki\\Extension\\YetiTweaks\\YetiTweaksHooks::onTitleSquidURLs"
	},
	"JobClasses": {
		"refreshLinksBatch": "MediaWiki\\Extension\\YetiTweaks\\RefreshLinksBatchJob"
	},
	"config_prefix": "wgYetiTweaks",
	"config": {
		"AddFooterLinks": {
			"value": true
		},
		"AnalyticsID": {
			"value": ""
		},
		"CentralDB": {
			"value": "yetidb"
		},
		"CFToken": {
			"value": ""
		},
		"CFZone": {
			"value": ""
		},
		"CSP": {
			"value": ""
		},
		"CSPAnons": {
			"value": ""
		},
		"EnableTheming": {
			"value": true
		},
		"EnableFavicon": {
			"value": false
		},
		"Favicon32x32": {
			"value": false
		},
		"Favicon16x16": {
			"value": false
		},
		"EnableMessageOverrides": {
			"value": true
		},
		"NoRobots": {
			"value": false
		},
		"SitemapsCompressed": {
			"value": true
		},
		"SitemapsFileBackend": {
			"value": null
		},
		"Themes": {
			"value": [
				"dark",
				"light"
			]
		},
		"UseSFS": {
			"value": true
		}
	},
	"AvailableRights": [
		"bypassgloopspam"
	],
	"manifest_version": 2
}
